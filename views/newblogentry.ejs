<form method='post' enctype='multipart/form-data' action='/admin/blogentries/new'>
	<ul>
	  <li class='formlist'>Title:</li>
	  <li class='formlist'><input type='text' name='title' size='100' maxlength='100'/></li>
	  <li class='formlist'>Content:</li>
	  <li class='formlist'><textarea name='content' rows='15' cols='100'></textarea></li>
	  <li class='formlist'>Image (optional):</li>
	  <li class='formlist'><input type='file' name='image'/></li>
	  <li class='formlist'><input type='submit' name='submit' value='submit'/></li>
	</ul>
</form>
<%- contentFor('contenttitle') %>
Submit a New Blog Entry
<%- contentFor('og_title') %>
Kristina Zakrzewski
<%- contentFor('og_description') %>
Author | Teachor | Mother | Wife
